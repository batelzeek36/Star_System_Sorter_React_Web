      "classical": {
        "hd_title": "Selfishness",
        "hd_quote": "The ego-driven first law of caring for oneself which is not necessarily at the expense of others."
      },
      "interpretation": {
        "keywords": [
          "self-care first",
          "boundaries",
          "resource intake"
        ],
        "behavioral_axis": "Prioritizes personal fuel so care is sustainable.",
        "shadow_axis": "Envy or martyrdom when depleted."
      },
      "relationship_needs": {
        "needs_space_to_refuel": true,
        "rejects_guilt_based_care": true,
        "requires_clear_boundaries": true
      },
      "group_role": "baseline provider (self-maintenance first)",
